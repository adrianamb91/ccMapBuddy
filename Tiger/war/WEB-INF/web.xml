<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Tiger</servlet-name>
		<servlet-class>com.cc.tiger.servlets.TigerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Tiger</servlet-name>
		<url-pattern>/tiger</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Setup</servlet-name>
		<servlet-class>com.cc.tiger.servlets.SetupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Setup</servlet-name>
		<url-pattern>/setup</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetFriends</servlet-name>
		<servlet-class>com.cc.tiger.servlets.GetFriendsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetFriends</servlet-name>
		<url-pattern>/getfriends</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetVisibleFriends</servlet-name>
		<servlet-class>com.cc.tiger.servlets.GetVisibleFriendsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetVisibleFriends</servlet-name>
		<url-pattern>/getvisiblefriends</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetFriendLocation</servlet-name>
		<servlet-class>com.cc.tiger.servlets.GetFriendLocationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetFriendLocation</servlet-name>
		<url-pattern>/getfriendlocation</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SaveLocation</servlet-name>
		<servlet-class>com.cc.tiger.servlets.SaveLocationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SaveLocation</servlet-name>
		<url-pattern>/savelocation</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
